        rdf:type  led:WebReference;
        <http://purl.org/dc/terms/date>
                "2015-01-20"^^<http://www.w3.org/2001/XMLSchema#date>;
        <http://purl.org/dc/terms/title>
                "Letters of composers : an anthology, 1603-1945 / compiled and edited by Gertrude Norman and Miriam Lubell Shrifte."@en;
