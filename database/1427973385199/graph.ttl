                <http://data.open.ac.uk/time/edtf/1955-uu-uu>;
        <http://purl.org/dc/terms/dateApproved>
                "2015-08-27T13:27:10.000Z"^^<http://www.w3.org/2001/XMLSchema#dateTime>;
        <http://purl.org/dc/terms/dateSubmitted>
                "2015-04-02T11:16:25.000Z"^^<http://www.w3.org/2001/XMLSchema#dateTime>;
